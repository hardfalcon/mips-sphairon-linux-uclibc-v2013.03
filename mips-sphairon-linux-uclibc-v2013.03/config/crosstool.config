CT_EXPERIMENTAL=y
CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_SAVE_TARBALLS=y
CT_WORK_DIR="${CT_TOP_DIR}"
# CT_INSTALL_DIR_RO is not set
CT_CONNECT_TIMEOUT=3
CT_PATCH_LOCAL_FALLBACK_BUNDLED=y
CT_LOCAL_PATCH_DIR="${CT_TOP_DIR}/patches"
CT_LOG_EXTRA=y
# CT_LOG_PROGRESS_BAR is not set
CT_ARCH_ARCH="mips32r2"
CT_ARCH_FLOAT_SW=y
CT_ARCH_mips=y
CT_STATIC_TOOLCHAIN=y
CT_TOOLCHAIN_PKGVERSION="${TC_ID_STRING}"
CT_TOOLCHAIN_BUGURL="${TC_BUG_URL}"
CT_TARGET_VENDOR="sphairon"
CT_KERNEL_linux=y
CT_KERNEL_V_2_6_32=y
CT_BINUTILS_V_2_22=y
CT_BINUTILS_FOR_TARGET=y
CT_CC_LANG_CXX=y
CT_CC_EXTRA_CONFIG_ARRAY="--disable-symvers"
CT_CC_GCC_SYSTEM_ZLIB=y
# CT_CC_CXA_ATEXIT is not set
CT_CC_GCC_DISABLE_PCH=y
CT_CC_GCC_mips_llsc=y
CT_CC_GCC_mips_synci=y
CT_CC_GCC_mips_plt=y
CT_LIBC_uClibc=y
CT_LIBC_UCLIBC_CONFIG_FILE="${CT_TOP_DIR}/samples/${CT_TARGET}/${CT_LIBC}.config"
CT_LIBC_UCLIBC_WCHAR=y
CT_DEBUG_gdb=y
